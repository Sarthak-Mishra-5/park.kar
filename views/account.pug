extends base
block head 
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Owner Dashboard
    link(rel='stylesheet' href='/css/account.css')
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css' rel='stylesheet')
    link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js')
    link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css')
    script(src='https://cdn.tailwindcss.com')
block content    
    .main-container
        .container.light-style.flex-grow-1.container-p-y
            h4.font-weight-bold.py-3.mb-4
                | Welcome Owner!!!
            .card.overflow-hidden
                .row.no-gutters.row-bordered.row-border-light
                    .col-md-3.pt-0
                        .list-group.list-group-flush.account-settings-links
                            a.list-group-item.list-group-item-action(data-toggle='list' href='#revenue') Revenue
                            a.list-group-item.list-group-item-action(data-toggle='list' href='#listingmanagement') Listing Management
                            a.list-group-item.list-group-item-action(data-toggle='list' href='#Transactionhistory') Transaction History
                            a.list-group-item.list-group-item-action(data-toggle='list' href='#reviews') Reviews and Ratings
                            a.list-group-item.list-group-item-action.active(data-toggle='list' href='#customerdetails') Customer Details
                    .col-md-9
                        .tab-content
                            #customerdetails.tab-pane.fade.active.show
                                .card-body
                                    label.form-label View Customer Details
                                    .rounded-lg.border.bg-card.text-card-foreground.shadow-sm(data-v0-t='card')
                                        .flex.flex-col.p-6.pb-4(class='space-y-1.5')
                                            h3.font-semibold.whitespace-nowrap.tracking-tight.text-2xl Jochi
                                            p.text-muted-foreground.text-sm
                                                | Customer since May 12, 2023
                                        .p-6.grid.gap-2
                                            .grid.grid-cols-1.gap-2(class='md:grid-cols-2')
                                                div
                                                    h3.font-semibold.whitespace-nowrap.tracking-tight.text-base Email
                                                    p.text-sm.text-muted-foreground Jochie@example.com
                                                div
                                                    h3.font-semibold.whitespace-nowrap.tracking-tight.text-base Phone
                                                    p.text-sm.text-muted-foreground 123456789
                                                div
                                                    h3.font-semibold.whitespace-nowrap.tracking-tight.text-base Number of Bookings
                                                    p.text-sm.text-muted-foreground 5 bookings
                                hr.border-dark.m-0
                            #Transactionhistory.tab-pane.fade
                                .card-body.pb-2
                                    label.form-label Earnings summary and transaction history
                                    .rounded-lg.border.bg-card.text-card-foreground.shadow-sm(data-v0-t='card')
                                        .flex.flex-col.p-6(class='space-y-1.5')
                                            h3.text-2xl.font-semibold.whitespace-nowrap.leading-none.tracking-tight Transaction History
                                            p.text-sm.text-muted-foreground Customer bookings and payments
                                        .p-6
                                            .relative.w-full.overflow-auto
                                                table.w-full.caption-bottom.text-sm
                                                    thead(class='tr border-b')
                                                        tr.border-b.transition-colors(class='hover:bg-muted/50 data-[state=selected]:bg-muted')
                                                            th.h-12.px-4.text-left.align-middle.font-medium.text-muted-foreground(class='[&:has([role=checkbox])]:pr-0') Customer
                                                            th.h-12.px-4.text-left.align-middle.font-medium.text-muted-foreground(class='[&:has([role=checkbox])]:pr-0') Status
                                                            th.h-12.px-4.text-left.align-middle.font-medium.text-muted-foreground(class='[&:has([role=checkbox])]:pr-0') Bookings
                                                            th.h-12.px-4.text-left.align-middle.font-medium.text-muted-foreground(class='[&:has([role=checkbox])]:pr-0') Date
                                                    tbody(class='tr border-0')
                                                        tr.border-b.transition-colors(class='hover:bg-muted/50 data-[state=selected]:bg-muted')
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') Saksham
                                                            td.p-4.align-middle.text-green-600(class='[&:has([role=checkbox])]:pr-0') Paid
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') 3
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') June 23, 2024
                                                        tr.border-b.transition-colors(class='hover:bg-muted/50 data-[state=selected]:bg-muted')
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') Dhruv
                                                            td.p-4.align-middle.text-red-600(class='[&:has([role=checkbox])]:pr-0') Unpaid
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') 1
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') June 23, 2024
                                                        tr.border-b.transition-colors(class='hover:bg-muted/50 data-[state=selected]:bg-muted')
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') Devender
                                                            td.p-4.align-middle.text-green-600(class='[&:has([role=checkbox])]:pr-0') Paid
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') 3
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') March 24, 2024
                                                        tr.border-b.transition-colors(class='hover:bg-muted/50 data-[state=selected]:bg-muted')
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') Jochi
                                                            td.p-4.align-middle.text-red-600(class='[&:has([role=checkbox])]:pr-0') Unpaid
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') 1
                                                            td.p-4.align-middle(class='[&:has([role=checkbox])]:pr-0') March 23, 2024
                                hr.border-dark.m-0
                            #revenue.tab-pane.fade
                                .card-body.pb-2
                                    .form-group.Revenue
                                        label.form-label Your Earnings
                                        .bookings
                                            .rounded-lg.border.bg-card.text-card-foreground.shadow-sm.w-full.max-w-sm.bookingscard(data-v0-t='card')
                                                .p-6.flex.flex-col.items-center.space-y-2
                                                    h3.text-2xl.font-semibold.whitespace-nowrap.leading-none.tracking-tight
                                                        | $1,235.00
                                                    p.text-sm.text-muted-foreground Revenue
                                                .p-6.flex.flex-col.items-center.space-y-2
                                                    .flex.items-center.space-x-2
                                                        svg.w-4.h-4.text-gray-500(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='dark:text-gray-400')
                                                            rect(width='18' height='18' x='3' y='4' rx='2' ry='2')
                                                            line(x1='16' x2='16' y1='2' y2='6')
                                                            line(x1='8' x2='8' y1='2' y2='6')
                                                            line(x1='3' x2='21' y1='10' y2='10')
                                                            path(d='m9 16 2 2 4-4')
                                                        span.text-sm.font-medium March 2024
                                                    .grid.grid-cols-2.w-full.gap-2
                                                        .text-sm.text-gray-500(class='dark:text-gray-400')
                                                            span Bookings
                                                        .text-sm.text-right.text-gray-500 1,234
                                hr.border-dark.m-0
                            #account-social-links.tab-pane.fade
                            #listingmanagement.tab-pane.fade
                                .card-body
                                    label.form-label Add, Edit, or Remove Parking listings
                                    .bookings
                                        .rounded-lg.border.bg-card.text-card-foreground.shadow-sm.w-full.max-w-sm(data-v0-t='card')
                                            .p-6.flex.flex-col.items-center.space-y-2
                                                h3.text-2xl.font-semibold.whitespace-nowrap.leading-none.tracking-tight Manage Parking
                                            .p-6.flex.flex-col.items-center.space-y-4
                                                .grid.gap-4.w-full.listmanage
                                                    .grid.grid-cols-2.gap-4.w-full
                                                        button.inline-flex.items-center.justify-center.whitespace-nowrap.text-sm.font-medium.ring-offset-background.transition-colors.border.border-input.bg-background.h-9.rounded-md.px-3(class='focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:pointer-events-none disabled:opacity-50 hover:bg-accent hover:text-accent-foreground')
                                                            svg.h-4.w-4.mr-2(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                                                path(d='M5 12h14')
                                                                path(d='M12 5v14')
                                                            |                                                     Add Parking
                                                        button.inline-flex.items-center.justify-center.whitespace-nowrap.text-sm.font-medium.ring-offset-background.transition-colors.border.border-input.bg-background.h-9.rounded-md.px-3(class='focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:pointer-events-none disabled:opacity-50 hover:bg-accent hover:text-accent-foreground hover:border-black')
                                                            svg.h-4.w-4.mr-2(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                                                path(d='M20 5H9l-7 7 7 7h11a2 2 0 0 0 2-2V7a2 2 0 0 0-2-2Z')
                                                                line(x1='18' x2='12' y1='9' y2='15')
                                                                line(x1='12' x2='18' y1='9' y2='15')
                                                            |                                                     Remove Parking
                                                        button.inline-flex.items-center.justify-center.whitespace-nowrap.text-sm.font-medium.ring-offset-background.transition-colors.border.border-input.bg-background.h-9.rounded-md.px-3.hover(class='focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:pointer-events-none disabled:opacity-50 hover: border-black:text-accent-foreground')
                                                            svg.h-4.w-4.mr-2(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                                                path(d='M14.5 4h-5L7 7H4a2 2 0 0 0-2 2v9a2 2 0 0 0 2 2h16a2 2 0 0 0 2-2V9a2 2 0 0 0-2-2h-3l-2.5-3z')
                                                                circle(cx='12' cy='13' r='3')
                                                            |                                                     Upload Photos
                                                        button.inline-flex.items-center.justify-center.whitespace-nowrap.text-sm.font-medium.ring-offset-background.transition-colors.border.border-input.bg-background.h-9.rounded-md.px-3(class='focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-ring focus-visible:ring-offset-2 disabled:pointer-events-none disabled:opacity-50 hover:bg-accent hover:text-accent-foreground')
                                                            svg.h-4.w-4.mr-2(xmlns='http://www.w3.org/2000/svg' width='24' height='24' viewbox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round')
                                                                path(d='M6 3h12')
                                                                path(d='M6 8h12')
                                                                path(d='m6 13 8.5 8')
                                                                path(d='M6 13h3')
                                                                path(d='M9 13c6.667 0 6.667-10 0-10')
                                                            |                                                     Specify Pricing
                                hr.border-dark.m-0
                            #reviews.tab-pane.fade
                                .card-body.pb-2
                                    .reviews
                                        label.form-label Feedback Of your Parking
                                        .d-flex.justify-content-left
                                            .content.text-center
                                                .ratings
                                                    span.product-rating 4.6
                                                    span /5
                                                    .stars
                                                        i.fa.fa-star
                                                        i.fa.fa-star
                                                        i.fa.fa-star
                                                        i.fa.fa-star
                                                    .rating-text
                                                        span 46 ratings &amp; 15 reviews
                                hr.border-dark.m-0
                            .text-right.mt-3
                                button.px-3.py-2.text-sm.font-medium.text-center.text-white.bg-blue-700.rounded-lg.save(type='button' class='hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800') Save changes
                                button.cancel(type='button') Cancel
    script(data-cfasync='false' src='/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js')
    script(src='https://code.jquery.com/jquery-1.10.2.min.js')
    script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/js/bootstrap.bundle.min.js')
    script(type='text/javascript')
